{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --include=dev && cd api && npm install && cd api && npx prisma generate",
  "cacheDirectories": [],
  "rewrites": [
    {
      "source": "/api/process-workflows-cron",
      "destination": "/api/process-workflows-cron"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/src/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/process-workflows-cron",
      "schedule": "* * * * *"
    }
  ],
  "functions": {
    "api/src/server.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/process-workflows-cron.js": {
      "maxDuration": 300,
      "memory": 2048
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}